        var success = dtl_check();
        if (success)
            $('a[num=' + $(".tabArea").tabs("option", "active") + ']').attr('must', 'N');
        else
            $('a[num=' + $(".tabArea").tabs("option", "active") + ']').attr('must', 'Y');
        return true;
    }

