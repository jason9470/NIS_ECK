       }
        @if (dt != null && dt.Rows.Count > 0)
    {
        int PlaceCnt = dt.AsEnumerable().Count(x => x.Field<string>("ITEMID").StartsWith("place_"));
        int CurrentPlace = 1;
        @:$('#maode').val('@ViewBag.mode');
                                    @:$('#tableid').val('@dt.Rows[0]["TABLEID"]');
                                    foreach (DataRow r in dt.Rows)
        {
            if (r["ITEMTYPE"].ToString() == "radio")
            {
                if (r["ITEMID"].ToString().Trim().IndexOf("place_") >= 0 && CurrentPlace <= PlaceCnt)
                {
                    @:set_for_rb('place_@(CurrentPlace.ToString())', '@r["ITEMVALUE"]');
                                                CurrentPlace += 1;
                }
                else
                {
                    @:set_for_rb('@r["ITEMID"]', '@r["ITEMVALUE"]');
                                            }
                 //如果為疼痛評估時

                if (r["ITEMID"].ToString() == "param_pain_assessment_assess")
                {
                    if (r["ITEMVALUE"].ToString() != "")
                    {
                        @:document.getElementById("param_pain_assessment_assess").innerText = ' (@(r["ITEMVALUE"].ToString()))';
                                    }
                }
            }
                else if (r["ITEMTYPE"].ToString() == "hidden" && r["ITEMID"].ToString() == "param_pain_assessment_record")
            {
                int ps_value = 0;
                    if (r["ITEMVALUE"].ToString() != "")
                    {
                        string vs_record = r["ITEMVALUE"].ToString();
                        //疼痛計分
                        Regex rgx = new Regex(@"^([(]\d+[)])|^(\d+)");
                        foreach (string ps in vs_record.ToString().Split('|'))
                        {
                            if (ps != "")
                            {
                                ps_value += int.Parse(rgx.Match(ps).ToString().Replace("(", "").Replace(")", ""));
                                @:document.getElementById("pain_level").innerText = '@(ps_value)分';
                            }
                        }
                    }
            }
            else if (r["ITEMTYPE"].ToString() == "checkbox")
            {
                @:set_for_ck('@r["ITEMID"]', '@r["ITEMVALUE"]');

                                                        }
             else if (r["ITEMTYPE"].ToString() == "ddl")
                {
                    @*if (r["ITEMID"].ToString() == "wound_type")//傷口
                    {
                        for (int i = 0; i < r["ITEMVALUE"].ToString().Split(',').Length - 1; i++)
                        {
                          @:$('#but_wound_ass').click();

                            }

                    }
                    if (r["ITEMID"].ToString() == "inHosp")//壓傷
                    {
                        for (int i = 0; i < r["ITEMVALUE"].ToString().Split(',').Length - 1; i++)
                        {
                          @:$('#but_wound_pre').click();

                            }

                    }*@
                    if (r["ITEMID"].ToString() == "Prompt_TubeKind")//管路
                    {
                        for (int i = 0; i < r["ITEMVALUE"].ToString().Split(',').Length - 1; i++)
                        {
                            @:$('#but_tube_div').click();
                                                    }
                    }
                    @:set_for_ddl('@r["ITEMID"]', '@r["ITEMVALUE"]');
                                        }
            else if (r["ITEMTYPE"].ToString() == "text" || r["ITEMTYPE"].ToString() == "textarea")
            {
                if (r["ITEMID"].ToString() == "param_med_name")
                {
                    for (int i = 0; i < r["ITEMVALUE"].ToString().Split(',').Length - 1; i++)
                    {
                    @:$('#btn_add_med').click();
                                                }
                }
                 //20240703
                  if (r["ITEMID"].ToString() == "param_travel_domestic_city" || r["ITEMID"].ToString() == "param_travel_domestic_viewpoint")
                  {
                       var data = r["ITEMVALUE"].ToString();
                       if(data != "")
                       {
                            @:$('.param_travel_domestic').show();
                        }
                  }
                  if (r["ITEMID"].ToString() == "param_travel_aboard_country" || r["ITEMID"].ToString() == "param_travel_aboard_destination")
                  {
                       var data = r["ITEMVALUE"].ToString();
                       if(data != "")
                       {
                          @:$('.param_travel_aboard').show();
                       }
                  }
                if (r["ITEMID"].ToString() == "param_EMGContact")
                {
                    for (int i = 0; i < r["ITEMVALUE"].ToString().Split(',').Length - 1; i++)
                    {
                    @:$('#btn_add_contact').click();
                                                }
                }
                if (r["ITEMID"].ToString() == "wound_date")//傷口
                    {
                        for (int i = 0; i < r["ITEMVALUE"].ToString().Split(',').Length - 1; i++)
                        {
                            @:$('#but_wound_ass').click();
                                                    }
                    }
                    if (r["ITEMID"].ToString() == "wound_pre_date")//壓傷
                    {
                        for (int i = 0; i < r["ITEMVALUE"].ToString().Split(',').Length - 1; i++)
                        {
                            @:$('#but_wound_pre').click();
                                                    }
                    }
                if (r["ITEMVALUE"].ToString().Split(',').Length > 0)
                {
                @:set_for_txt('@r["ITEMID"]', '@r["ITEMVALUE"]');
                                                            }
                else
                {
                @:$('#@r["ITEMID"]').val('@r["ITEMVALUE"].ToString()');
                                                            }
                                              //如果為營養評估時
                    if (r["ITEMID"].ToString() == "param_nutrition_totle")
                    {
                        if (r["ITEMVALUE"] != "")
                        {
                          int  totle =Convert.ToInt16(r["ITEMVALUE"]);

                            if (totle < 3) {
                                @:document.getElementById("nutrition_sores").innerText = '(MUST小於3分，暫時不需要進一步的營養評估)';
                                            }
                            if (totle >= 3) {
                                @:document.getElementById("nutrition_sores").innerText = '(MUST≧3分為營養不良高度風險，通知主治醫師決定是否照會營養師做進一步的營養評估)';
                                            }
                        }

                    }
            }
            else if (r["ITEMTYPE"].ToString() == "hidden" && r["ITEMID"].ToString() == "check_tab_val")
            {
                @:set_a_must('@r["ITEMVALUE"].ToString()');
                                        }
        }
        @:CPDAmount();

                 //   if (pf.Age > 5)
        {
            @:con_bmi();
                                    }
        }
</script>
