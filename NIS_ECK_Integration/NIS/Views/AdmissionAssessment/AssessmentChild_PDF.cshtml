    function ChangeDateCont(pCtrl, pPanel, pCalendar) {
        var countid = parseInt($(pCtrl).attr("countid"));
        $(pPanel + countid).empty();
        $(pPanel + countid).append("<input type='text' name='" + pCalendar + "' id='" + pCalendar + "_" + countid + "' />");
