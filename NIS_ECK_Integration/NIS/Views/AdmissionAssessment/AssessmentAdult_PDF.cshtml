    //綁訂日期
    function MultiSetDateTime(DateField, TimeField) {
        var maxDate = new Date();
        maxDate.setDate(maxDate.getDate());
        $("#" + DateField).removeAttr('readonly');
        $("#" + DateField).attr("readonly", true);
        $("#" + DateField).css({ "width": "80px" });
        $("#" + DateField).css({ "font-size": "12px" });
        if (TimeField != null) {
            $("#" + TimeField).removeAttr('readonly');
            $("#" + TimeField).attr("readonly", true);
            $("#" + TimeField).css({ "width": "50px" });
            $("#" + TimeField).css({ "font-size": "12px" });
            $("#" + DateField).datetimepicker({
                altField: "#" + TimeField,
                beforeShowDay: function (date) {
                    switch (date.getDay()) {
                        case 6:
                            return [true, 'satday'];
                            break;
                        case 0:
                            return [true, 'sunday'];
                            break;
                        default:
                            return [true, ''];
                            break;
                    }
                },
                inline: true,
                showAnim: "slideDown",
                controlType: myControl,
                maxDateTime: maxDate,
                addSliderAccess: true,
                sliderAccessArgs: { touchonly: false }
            });
        } else {
            $("#" + DateField).datetimepicker({
                showTimepicker: false,
                beforeShowDay: function (date) {
                    switch (date.getDay()) {
                        case 6:
                            return [true, 'satday'];
                            break;
                        case 0:
                            return [true, 'sunday'];
                            break;
                        default:
                            return [true, ''];
                            break;
                    }
                },
                inline: true,
                showAnim: "slideDown",
                controlType: myControl,
                addSliderAccess: true,
                sliderAccessArgs: { touchonly: false }
            });
        }

    }

