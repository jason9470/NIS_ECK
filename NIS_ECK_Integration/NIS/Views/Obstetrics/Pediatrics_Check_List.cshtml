                                        </td>
                                    </tr>
                                    <tr>
                                        <th>
                                            臍帶
                                            <br />
                                            <button type="button" onclick="clearValue('TXT_UC_D');">清空</button>
                                        </th>
                                        <td>
                                            <input type="radio" name="RB_UC" value="0" />未脫落
                                            <br />
                                            <input type="radio" name="RB_UC" value="1" />脫落
                                            ，領回日期
                                            @if (r["UC_D"].ToString() != "")
                                            {
                                                @Html.TextBox("TXT_UC_D", Convert.ToDateTime(r["UC_D"].ToString()).ToString("yyyy/MM/dd"), new { style = "width:80px", @readonly = "readonly" })
                                            }
                                            else
                                            {
                                                @Html.TextBox("TXT_UC_D", "", new { style = "width:80px", @readonly = "readonly" })
                                            }
                                            @WebTool.setDateTime("TXT_UC_D", "", "", "30")
                                            <script>
                                                @if(r["UC"].ToString() != "")
                                                {
                                                    @:set_for_rb("RB_UC", @r["UC"]);
                                                }
                                            </script>
                                        </td>
                                    </tr>
                                    <tr>
                                        <th>兒童健康手冊</th>
                                        <td>
                                            <input type="checkbox" name="CK_CHM" value="1" />已備

                                        </td>
                                    </tr>

                                    <tr>
                                        <th>早產兒套書</th>
                                        <td>
                                            <input type="radio" name="RB_PBS" value="0" />已給予
                                            <br />
                                            <input type="radio" name="RB_PBS" value="1" />不需給予
                                            <script>
                                                @if(r["PBS"].ToString() != "")
                                                {
                                                    @:set_for_rb("RB_PBS", @r["PBS"]);
                                                }
                                            </script>
                                        </td>
                                    </tr>
                                    <tr>
                                        <th>奶嘴</th>
                                        <td>
                                            <input type="checkbox" name="CK_NIP" value="0" />自備
                                            @Html.TextBox("TXT_NIP_S", r["NIP_S"], new { style = "width:100px" })
                                            <input type="checkbox" name="CK_NIP" value="1" />醫院
                                            @Html.TextBox("TXT_NIP_H", r["NIP_H"], new { style = "width:100px" })
                                            <script>
                                            @if (r["NIP"].ToString() != "")
                                            {
                                                var NIP = r["NIP"].ToString();
                                                var NIPV = new List<string>();

                                                for (var i = 0; i < NIP.ToString().Length; i++)
                                                {
                                                    if (NIP[i] == '1')
                                                    {
                                                        NIPV.Add(i.ToString());
                                                    }
                                                }
                                                @:set_for_ck('CK_NIP', '@String.Join(",", NIPV)');
                                            }
                                            </script>
                                        </td>
                                    </tr>
                                    <tr>
                                        <th class="head">14</th>
                                        <th>自備用物</th>
                                        <td>
                                            @Html.TextArea("TXT_SR", r["SR"].ToString(), new { style = "width:100%", @rows = "5" })
                                        </td>
                                    </tr>
                                </table>
                                <script>
                                            @if (r["CHM"].ToString() == "1")
                                            {
                                                @:set_for_ck('CK_CHM', '1');
                                            }
                                </script>
                            </div>
                        </td>
                    </tr>
                </table>
            }
        </div>
    </form>
}

<script>
    $('form').on('keyup keypress', function (e) {
        var keyCode = e.keyCode || e.which;
        if (keyCode === 13) {
            e.preventDefault();
            return false;
        }
    });
</script>
