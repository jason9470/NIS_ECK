@{
    ViewBag.Title = "Pediatrics_Check_List";
    Layout = "~/Views/Layout/FormLayout.cshtml";
}
<style>
    div.addForm tr {
        text-align: left !important;
    }
</style>
<link href="~/Content/StyleSheet/BabySystem.css" rel="stylesheet" />

<script>
    $(function () {
        setGridColor();
        //設定高度
        $(window).resize(function () {
            var height = $(this).outerHeight();
            if (height <= 300)
                height = 450;
            $(".dataArea").outerHeight(height - 300);
        }).trigger("resize");
    });

    //格行換色
    function setGridColor() {
        $('.dataArea tr:odd').css('background', '#ABC8E2');
        $('.dataArea tr:even').css('background', '#E1E6FA');

        $('.head').css('background', '#85A5CC');
    }

    function set_for_rb(rb_name, dvalue) {
        if (dvalue != 'undefined') {
            var rb = document.getElementsByName(rb_name);
            for (var i = 0; i < rb.length; i++) {
                if (rb[i].value == dvalue)
                    $(rb[i]).click();
            }
        }
    }

    function set_for_ck(ck_name, val) {
        if (val != '') {
            var default_val = val.split(",");
            var ck = document.getElementsByName(ck_name);
            for (var i = 0; i < default_val.length; i++) {
                for (var j = 0; j < ck.length; j++) {
                    if (default_val[i] == ck[j].value)
                        $(ck[j]).click();
                }
            }
        }
    }

    function SPC_N_Checked() {
        var val = $('input:checkbox:checked[name="CK_SPC_N"]').map(function () { return $(this).val(); }).get();
        if (val == '1') {
            set_for_rb('RB_SPC_LGE', '0');
            set_for_rb('RB_SPC_AUS', '0');
            set_for_rb('RB_SPC_HUS', '0');
            set_for_rb('RB_SPC_BUS', '0');
            set_for_rb('RB_SPC_ECG', '0');
            set_for_rb('RB_SPC_AABR', '0');

            $("input[name=RB_SPC_LGE]").attr('disabled', true);
            $("input[name=RB_SPC_AUS]").attr('disabled', true);
            $("input[name=RB_SPC_HUS]").attr('disabled', true);
            $("input[name=RB_SPC_BUS]").attr('disabled', true);
            $("input[name=RB_SPC_ECG]").attr('disabled', true);
            $("input[name=RB_SPC_AABR]").attr('disabled', true);
        }
        else {
            $("input[name=RB_SPC_LGE]").attr('checked', false).attr('disabled', false);
            $("input[name=RB_SPC_AUS]").attr('checked', false).attr('disabled', false);
            $("input[name=RB_SPC_HUS]").attr('checked', false).attr('disabled', false);
            $("input[name=RB_SPC_BUS]").attr('checked', false).attr('disabled', false);
            $("input[name=RB_SPC_ECG]").attr('checked', false).attr('disabled', false);
            $("input[name=RB_SPC_AABR]").attr('checked', false).attr('disabled', false);
        }
    }

    function SPS_N_Checked() {
        var val = $('input:checkbox:checked[name="CK_SPS_N"]').map(function () { return $(this).val(); }).get();
        if (val == '1') {
            set_for_rb('RB_SPS_PD', '0');
            set_for_rb('RB_SPS_SCID', '0');
            set_for_rb('RB_SPS_5IN1', '0');
            set_for_rb('RB_SPS_ALD', '0');
            set_for_rb('RB_SPS_SHL', '0');
            set_for_rb('RB_SPS_CCHS', '0');
            set_for_rb('RB_SPS_CMV', '0');

            $("input[name=RB_SPS_PD]").attr('disabled', true);
            $("input[name=RB_SPS_SCID]").attr('disabled', true);
            $("input[name=RB_SPS_5IN1]").attr('disabled', true);
            $("input[name=RB_SPS_ALD]").attr('disabled', true);
            $("input[name=RB_SPS_SHL]").attr('disabled', true);
            $("input[name=RB_SPS_CCHS]").attr('disabled', true);
            $("input[name=RB_SPS_CMV]").attr('disabled', true);
        }
        else {
            $("input[name=RB_SPS_PD]").attr('checked', false).attr('disabled', false);
            $("input[name=RB_SPS_SCID]").attr('checked', false).attr('disabled', false);
            $("input[name=RB_SPS_5IN1]").attr('checked', false).attr('disabled', false);
            $("input[name=RB_SPS_ALD]").attr('checked', false).attr('disabled', false);
            $("input[name=RB_SPS_SHL]").attr('checked', false).attr('disabled', false);
            $("input[name=RB_SPS_CCHS]").attr('checked', false).attr('disabled', false);
            $("input[name=RB_SPS_CMV]").attr('checked', false).attr('disabled', false);
        }
    }

    function check_for_datetime(obj, date, time, dtValue) {

        var name = obj.attributes.name.nodeValue;
        var ck = $(obj).is(":checked")
        var val = $(obj).val();

        var dateField = $("#" + date).val();
        if (time)
            var timeField = $("#" + time).val();

        if (name.indexOf('CK') >= 0) {
            if (ck && !dtValue) {
                var now = new Date();

                $("#" + date).val(now.getFullYear() + "/" + paddingLeft(Number(now.getMonth() + 1).toString(), 2) + "/" + paddingLeft(now.getDate().toString(), 2));
                if (time)
                    $("#" + time).val(paddingLeft(Number(now.getHours()).toString(), 2) + ":" + paddingLeft(Number(now.getMinutes()).toString(), 2));
            }
            else if (ck && dtValue && !dateField && !timeField) {
                var now = new Date();

                $("#" + date).val(now.getFullYear() + "/" + paddingLeft(Number(now.getMonth() + 1).toString(), 2) + "/" + paddingLeft(now.getDate().toString(), 2));
                if (time)
                    $("#" + time).val(paddingLeft(Number(now.getHours()).toString(), 2) + ":" + paddingLeft(Number(now.getMinutes()).toString(), 2));
            }
            else if (ck && dtValue && dateField && timeField) {
            }
            else {
                $("#" + date).val('');
                if (time)
                    $("#" + time).val('');
            }
        } else {
            if (val === "1" && !dtValue) {
                var now = new Date();

                $("#" + date).val(now.getFullYear() + "/" + paddingLeft(Number(now.getMonth() + 1).toString(), 2) + "/" + paddingLeft(now.getDate().toString(), 2));
                if (time)
                    $("#" + time).val(paddingLeft(Number(now.getHours()).toString(), 2) + ":" + paddingLeft(Number(now.getMinutes()).toString(), 2));
            }
            else if (val === "1" && dtValue && !dateField && !timeField) {
                var now = new Date();

                $("#" + date).val(now.getFullYear() + "/" + paddingLeft(Number(now.getMonth() + 1).toString(), 2) + "/" + paddingLeft(now.getDate().toString(), 2));
                if (time)
                    $("#" + time).val(paddingLeft(Number(now.getHours()).toString(), 2) + ":" + paddingLeft(Number(now.getMinutes()).toString(), 2));
            }
            else if (val === "1" && dtValue && dateField && timeField) {
            }
            else {
                $("#" + date).val('');
                if (time)
                    $("#" + time).val('');
            }
        }
    }

    function paddingLeft(str, length) {
        if (str.length >= length)
            return str;
        else
            return paddingLeft("0" + str, length);
    }

    function updateLastTime(dateName) {
        var date = $("#TXT_" + dateName + "_D_DAY").val();
        var time = $("#TXT_" + dateName + "_D_TIME").val();

        if (date && time) {
            $("#TXT_" + dateName + "_LD_DAY").val(date);
            $("#TXT_" + dateName + "_LD_TIME").val(time);

            $("#TXT_" + dateName + "_D_DAY").val('');
            $("#TXT_" + dateName + "_D_TIME").val('');
        }
    }

    function getBabyLab(obj) {
        var date = $(obj).val();

        if (date != "" && date != undefined) {
            $.ajax({
                url: "GetBabyLab",
                type: "GET",
                data: { date: date },
                success: function (data, textStatus, jqXHR) {
                    if (data != "") {
                        $("#TXT_SOB_R").val(data);
                    }
                },
                error: function (err) {
                },
                dataType: 'json'
            });
        }
    }

    function clearValue(name) {
        $('input[name^=' + name + ']').val('');
    }
</script>
