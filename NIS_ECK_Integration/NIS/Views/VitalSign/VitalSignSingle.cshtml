        if ($("#bph_text").val() != '' && $("#bpl_text").val != '') {
            var result = ($("#bph_text").val() / 3 + ($("#bpl_text").val() / 3 * 2));
            resultFixed = result.toFixed(2)
            document.getElementById('bpm').innerText = "(" + resultFixed + ") mmHg";
        }
