        var type = $(obj).closest('tr').find('input[name=vs_item]').val();
        if ($(obj).prop('checked')) {
            $('input[type=text][name=' + type + '_record]').val('').hide();
            $('#VS_plan_' + type).attr("checked", true);
        }
        else
            $('input[type=text][name=' + type + '_record]').show();
    }


