@using System.Data;
@{
    ViewBag.Title = "Record";
    Layout = "~/Views/Layout/FormLayout.cshtml";
    string title = "CVVH-新增內容";
    string M_rc_id = ViewBag.id;
    string M_rc_time = ViewBag.dtldate;
    DataTable dt = ViewBag.dt;
    string feeno = ViewBag.FeeNo;
    string RootDocument = ViewBag.RootDocument;
    double Todaytotal = Convert.ToDouble(ViewBag.white) + Convert.ToDouble(ViewBag.night) + Convert.ToDouble(ViewBag.longnight);
    NIS.Data.UserInfo userInfo = (NIS.Data.UserInfo)Session["UserInfo"];
    string employeesNo = userInfo.EmployeesNo.ToString();
    NIS.Data.PatientInfo pf = (NIS.Data.PatientInfo)Session["PatInfo"];
    //日期選擇器，鎖定時間
    NIS.Data.Complement_List complement_List = (NIS.Data.Complement_List)Session["Complement_List"];

    bool IsDischarged = false;
    if (Convert.ToDateTime(pf.OutDate).ToString("yyyy").ToString() != "0001" && pf.OutDate < DateTime.Now)
    {
        IsDischarged = true;
    }
}
<style type="text/css">
    .dataArea td {
        border-bottom: 1px solid rgba(20%,20%,40%,0.5);
        text-align: left;
    }

    #module_define {
        padding: 5px 0px 3px 0px;
        text-align: center;
        background-color: #9999FF;
        width: 100%;
        border: 1px solid #BBBBBB;
        border-right: 2px solid #999999;
        border-bottom: 2px solid #999999;
    }

    .funcArea LEGEND {
        margin-left: 5px;
        background: #FFF;
        padding: 3px 7px 2px 7px;
        border-top: none;
        border-left: none;
        border-right: none;
        border-bottom: none;
        font-size: 14px;
    }
</style>
<script type="text/javascript">
    var dtCount = '@(ViewBag.dtCount)';
    $(document).ready(function () {
        BFR();
        setGridColor();
        disbtn();
    });

    function setGridColor() {
        //格行換色
        $('.dataArea tr:odd').css('background', '#ABC8E2');
        $('.dataArea tr:even').css('background', '#E1E6FA');
    }

    //帶入最後一筆的BLOOD_FLOW_RATE記錄資料
    function BFR() {
        if (dtCount != "0") {
            $("#txt_blood_flow_rate").val($("#lab_BLOOD_FLOW_RATE_" + dtCount).text());
        }
    }

    //disabled按紐、前一日後一日
    function disbtn() {
        var beforeday = '@(ViewBag.beforeday)';
        var nextday = '@(ViewBag.nextday)';
        if (parseInt(beforeday) != 0) {
            $("#btn_before_day").attr("disabled", false);
            //$("#btn_before_day").css("color", '#000');//#d0d0d0
        } else {
            $("#btn_before_day").attr("disabled", true);
            //$("#btn_before_day").css("color", '#d0d0d0');
        }
        if (parseInt(nextday) != 0) {
            $("#btn_next_day").attr("disabled", false);
            // $("#btn_next_day").css("color", '#000');
        }
        else {
            $("#btn_next_day").attr("disabled", true);
            // $("#btn_next_day").css("color", '#d0d0d0');
        }
    }

    //即時計算
    function InstantCalculations() {
        var I_total = (parseFloat($("#txt_replace_fluid_ab").val()) + parseFloat($("#txt_flushes").val()) - parseFloat($("#txt_uf").val())).toFixed(2);
        $("#txt_io_total").val(I_total);
        var tempflag = (I_total > 0) ? "+" : "";
        $("#lab_io_total").text(tempflag + I_total);
    }

    //驗證欄位_數字
    function numcheck(obj) {
        var success = true;
        if (obj.value.indexOf('.') > -1)
            var re = /^[0-9]+\.[0-9]*$/;
        else
            var re = /^[0-9]+$/;
        if (!re.test(obj.value) && obj.value != "") {
            alert("只能輸入數字");
            $(obj).val('0');
            success = false;
        }
        return success;
    }

    function numiototalcheck(obj) {
        var success = true;
        if (isNaN(obj.value)) {
            alert("只能輸入數字");
            $(obj).val('0');
            success = false;
        } else {
            success = true;
        }
        return success;
    }

    function all_clear() {
        $('#txt_blood_flow_rate').val('0');
        $('#txt_venous_pressure').val('0');
        $('[name=rb_air_detector]')[0].checked = true;
        $('[name=rb_clots_in_venous_trap]')[0].checked = true;
        $('#txt_heparin').val('0');
        $('#txt_replace_fluid_ab').val('0');
        $('#txt_flushes').val('0');
        $('#txt_uf').val('0');
        $('#txt_replace_fluid_ab').val('0');
        $('#txt_io_total').val('0');
        $('#lab_io_total').text('0');
        $("#legend1").html("新增CVVH 內容");
        $('#form1').attr('action', 'InsertItem');
        BFR();
    }

    function edit(obj) {
        $("#legend1").html("修改CVVH 內容");
        var row = $(obj).attr('rownum');
        $("#record_start_date").val($("#lab_date_" + row).text());
        $("#record_start_time").val($("#lab_time_" + row).text());
        $("#txt_blood_flow_rate").val($("#lab_BLOOD_FLOW_RATE_" + row).text());
        $("#txt_venous_pressure").val($("#lab_VENOUS_PRESSURE_" + row).text());
        var tempair = $("#lab_AIR_DETECTOR_" + row).attr("tempstr");
        $("[name=rb_air_detector][value='" + tempair + "']").prop("checked", true);
        var temptrap = $("#lab_CLOTS_IN_VENOUS_TRAP_" + row).attr("tempstr");
        $("[name=rb_clots_in_venous_trap][value='" + temptrap + "']").prop("checked", true);
        $("#txt_heparin").val($("#lab_HEPARIN_" + row).text());
        $("#txt_replace_fluid_ab").val($("#lab_I_REPLACE_FLUID_" + row).text());
        $("#txt_flushes").val($("#lab_I_FLUSHES_" + row).text());
        $("#txt_uf").val($("#lab_O_UF_" + row).text());
        $("#hid_recordid").val($("#hid_record_" + row).val());
        $("#txt_io_total").val($("#lab_IO_TOTAL_" + row).text());
        $("#lab_io_total").text($("#lab_IO_TOTAL_" + row).text());
    }

    //刪除----
    function DelRecord(obj) {
        var Confirm = confirm("確定刪除嗎?");
        if (Confirm == true) {
            var dtlid = $(obj).attr('dtlid');
            var Mrcid = '@(M_rc_id)';
            var Mrctime = '@(M_rc_time)';
            loop_windows("open_cover");
            $.ajax({
                type: "POST",
                url: '@Url.Content("~/CVVHManage/DelRecordDtl")',
                dataType: 'Html',
                data: { id: Mrcid, dtl_id: dtlid },
                cache: false,
                success: function (response) {
                    if (response == "Y") {
                        alert("刪除成功");
                        // 重load該頁
                        window.location.href = "../CVVHManage/Record?id=" + Mrcid + "&dtldate=" + Mrctime + "";
                    } else {
                        alert("刪除失敗");
                    }
                }, complete: function () {
                    loop_windows("close_cover");
                }
            });
        } else {
            return;
        }

    }

    //計算I/O
    function ClassTotal() {
        var date = $("#record_start_date").val() + " " + $("#record_start_time").val();
        var testval = [];
        var arrayvalue = [];
        $("input[type='checkbox'][name='cb_class_total[]']:checked").each(function () {
            testval.push($(this).val());
            arrayvalue.push($(this).attr('temptotal'));
            $(this).prop("checked", false);
        });
        if (testval.length != 0) {
            var Ptestval = testval.join(',');
            var Parrayvalue = arrayvalue.join(',');
            $.ajax({
                type: "POST",
                url: '@Url.Content("~/CVVHManage/TakeCareRecord")',
                dataType: 'Html',
                data: { Ptype: Ptestval, Pdate: date, values: Parrayvalue },
                cache: false,
                success: function (response) {
                    if (response == "Y") {
                        alert("帶入護理記錄成功");
                    } else {
                        alert("帶入護理記錄失敗");
                    }
                }, complete: function () {
                }
            });
        } else {
            alert("請選擇轉護理記錄的班別!");
        }
    }

    function PrintPDF() {
        window.open("@(RootDocument)/Print/GetPDF?url=@(RootDocument)/CVVHManage/PrintList?feeno=@(feeno)&dtldate=@(Convert.ToDateTime(M_rc_time).ToString("yyyy/MM/dd"))&id=@(M_rc_id)&filename=test");

    }
    function save()
    {
        var setStr = "SET0410112653";
        var id = $("#MRecordid").val();
        var date = $("#record_start_date").val() + " " + $("#record_start_time").val();

          $.ajax({
              type: "POST",
              url: '@Url.Content("~/CVVHManage/InsertItem")',
              data: $("#form1").serialize(),
              cache: false,
              success: function (response) {
                  if (response == "Y") {
                      alert("新增成功");

                    if ('@IsDischarged' == 'False' ) {
                        window.open('../Billing/index?set=' + setStr, '', 'menubar=no,status=no,scrollbars=yes,top=0,left=300,toolbar=no,width=1290,height=800');
                    }

                      window.location.href = "../CVVHManage/Record?id=" + id + "&dtldate=" + date;
                  } else {
                      alert("新增失敗");
                  }
              }, complete: function () {
              }
          });

    }
</script>
