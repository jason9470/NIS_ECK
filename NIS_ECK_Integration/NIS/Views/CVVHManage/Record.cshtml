                <div class="dataArea" style="height: 110px;">
                    <table border="0" cellpadding="4" cellspacing="1" width="100%" style="width: 100%; background-color: #FFFFFF; font-size: 14px;">
                        @if (dt != null)
                        {
                           
                            <tbody>
                                @foreach (DataRow r in dt.Rows)
                                {
                                    string a = r["CREATNO"].ToString();
                                    <tr>
                                        <td style="width: 6%;text-align:center;">
                                            <label id="lab_date_@(r["ROWNUM"])" style="display:none;">@(Convert.ToDateTime(r["DATA_TIME"]).ToString("yyyy/MM/dd"))</label>
                                            <label id="lab_time_@(r["ROWNUM"])">@(Convert.ToDateTime(r["DATA_TIME"]).ToString("HH:mm"))</label>
                                        </td>
                                        <td style="width: 8%;"><label id="lab_BLOOD_FLOW_RATE_@(r["ROWNUM"])">@r["BLOOD_FLOW_RATE"]</label></td>
                                        <td style="width: 8%;"><label id="lab_VENOUS_PRESSURE_@(r["ROWNUM"])">@r["VENOUS_PRESSURE"]</label></td>
                                        @{
                                    string[] AirDetectorArray = { "OK", "NOT OK" };
                                    string[] ClotsInVenousTrapArray = { "+", "-", "±" };
                                        }
                                        <td style="width: 10%;"><label id="lab_AIR_DETECTOR_@(r["ROWNUM"])" tempstr="@(r["AIR_DETECTOR"])">@(AirDetectorArray[Convert.ToInt32(r["AIR_DETECTOR"])])</label></td>
                                        <td style="width: 7%;"><label id="lab_CLOTS_IN_VENOUS_TRAP_@(r["ROWNUM"])" tempstr="@(r["CLOTS_IN_VENOUS_TRAP"])">@(ClotsInVenousTrapArray[Convert.ToInt32(r["CLOTS_IN_VENOUS_TRAP"])])</label></td>
                                        <td style="width: 8%;"><label id="lab_HEPARIN_@(r["ROWNUM"])">@r["HEPARIN"]</label></td>
                                        <td style="width: 12%;"><label id="lab_I_REPLACE_FLUID_@(r["ROWNUM"])">@r["I_REPLACE_FLUID"]</label></td>
                                        <td style="width: 6%;"><label id="lab_I_FLUSHES_@(r["ROWNUM"])">@r["I_FLUSHES"]</label></td>
