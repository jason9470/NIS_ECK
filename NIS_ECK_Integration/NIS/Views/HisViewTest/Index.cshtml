@{
    Layout = "~/Views/Layout/FormLayout.cshtml";
    string feeno = ViewBag.feeno;
    string ExecFlag = ViewBag.ExecFlag;
    string TypeFlag = ViewBag.TypeFlag;
    string RootDocument = "";
    if (global_asax.localIp.Substring(0, 7) == "172.20.") //for ECK
    {
        RootDocument = ViewBag.RootDocument + "/NIS";
    }
    else if (NIS.Controllers.BaseController.debug_mode)
    { //for localhost
        RootDocument = ViewBag.RootDocument;
    }
    else //for demo
    {
        RootDocument = ViewBag.RootDocument + "/NIS_ECK";
    }
    string ChartNo = ViewBag.ChartNo;
}

