<script type="text/javascript">
 $(document).ready(function () {
        //loadMedPage();
    });



 function load_func(func_name) {
     var url_obj = "";
        $.ajax({//LOAD一個session
            type: 'post',
            url: '@(RootDocument)/HISView/WebS_ChangPtInfo',
            async: false,
            data: { 'strFeeNo': '@(feeno)' },
            success: function (data) {
            }
        });
        if (func_name == "IO") {
            func_name = "Index";
            url_obj = "&type=nis";
        }
        if (func_name == "IO_OBS") {
            func_name = "Index";
            url_obj = "&type=obs";
        }

     var control = "HISVIEW/";
        if (func_name.indexOf("VoiceInput") != -1) {
            //func_name = "VoiceInput"
            control = "HISVIEWTEST/"
     }
     if (func_name.indexOf("OBS_") >= 0) {
         func_name = func_name.toString().substring(4);
         control = "Obstetrics/"
     }
     if (func_name.indexOf("BABYBORN_") >= 0) {
         func_name = func_name.toString().substring(9);
         control = "BabyBorn/"
     }


        window.open("@(RootDocument)/" + control + func_name + "?feeno=" + '@(feeno)' + "&env=223&chartno=" + '@(ChartNo)' + "&IsHisView=true" + url_obj, 'TopMenu');
    }


</script>

