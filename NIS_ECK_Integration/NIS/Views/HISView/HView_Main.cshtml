@{
    Layout = null; 
}
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>護理資訊系統_NISView</title>
</head>
   @if (ViewBag.feeno != null || ViewBag.costcode != null)
   {
        <Frameset cols="180,*" frameborder="0">
            <Frame Src="./HView_PatList?costcode=@ViewBag.costcode" name="left" noresize="noresize">
            <Frameset rows="120,*" border=0>
                <Frame Src="./HView_Function?feeno=@ViewBag.feeno" name="top">
                <Frame Src="" name="right" id="fright">
            </Frameset>
        </Frameset>
    }
    else
    {
    <body style="text-align:center;">
        <h2>無輸入參數！</h2>
    </body>
    }
</html>

