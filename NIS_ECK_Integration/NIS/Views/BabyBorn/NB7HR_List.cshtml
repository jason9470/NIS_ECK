<link href="~/Content/StyleSheet/BabySystem.css" rel="stylesheet" />
<script>
    $(function () {
        setGridColor();
    });

    //格行換色
    function setGridColor() {
        var tr = $('.dataArea tr');
        var color = '#ABC8E2';
        for (var i = 0; i < tr.length; i++) {
            if ($(tr[i]).css('display') != 'none') {
                $(tr[i]).css('background', color);
                if (color == '#ABC8E2')
                    color = '#E1E6FA';
                else
                    color = '#ABC8E2';
            }
        }
        $('.head').css('background', '#85A5CC');
    }

    function set_for_rb(rb_name, dvalue) {
        if (dvalue != 'undefined') {
            var rb = document.getElementsByName(rb_name);
            for (var i = 0; i < rb.length; i++) {
                if (rb[i].value == dvalue)
                    $(rb[i]).click();
            }
        }
    }

    function set_for_ck(ck_name, val) {
        if (val != '') {
            var default_val = val.split(",");
            var ck = document.getElementsByName(ck_name);
            for (var i = 0; i < default_val.length; i++) {
                for (var j = 0; j < ck.length; j++) {
                    if (default_val[i] == ck[j].value)
                        $(ck[j]).click();
                }
            }
        }
    }

    function get_employee_name(obj, name) {
        var userno = $(obj).attr('id');
        $.ajax({
            url: "Get_Employee_Name",
            type: "GET",
            data: { userno: document.getElementById(userno).value },
            success: function (data, textStatus, jqXHR) {
                var userNameId = userno.replace("TXT_" + name, "LB_" + name);
                if (data == "") {
                    alert("查無此執行者員工編號!");
                    $("#" + userNameId).html('請輸入員工編號');
                }
                else {
                    $("#" + userNameId).html(data);
                }
            },
            error: function (err) {
            },
            dataType: 'text'
        });
    }

    function check(type, isAdd) {
        if (!isAdd) {
            id = $('input[name=IID_' + type + ']').val();
        } else {
            id = '';
        }

        if (!$('input[name=TXT_ASMT_TIME_DAY_' + type + id + ']').val() || !$('input[name=TXT_ASMT_TIME_TIME_' + type + id + ']').val()) {
            alert("[評估時間]未填")
            return false;
        }

        if (!$('input[name=TXT_TPR_' + type + id + ']').val()) {
            alert("[體溫/脈搏/呼吸]未填")
            return false;
        }
        if (!$('input[name=CK_BRE_TYP_' + type + id + ']:checked').val()) {
            alert("[呼吸狀態]未填")
            return false;
        }
        if (!$('input[name=RB_CRY_' + type + id + ']:checked').val()) {
            alert("[哭聲]未填")
            return false;
        }
        if ($('input[name=RB_CRY_' + type + id + ']:checked').val() != undefined && $('input[name=RB_CRY_' + type + id + ']:checked').val() == "4") {
            if (!$('input[name=TXT_CRY_NO_' + type + id + ']').val()) {
                alert("[哭聲-無]未填")
                return false;
            }
        }
        if (!$('input[name=RB_ACTI_' + type + id + ']:checked').val()) {
            alert("[活動力]未填")
            return false;
        }
        if (!$('input[name=RB_SKIN_' + type + id + ']:checked').val()) {
            alert("[膚色]未填")
            return false;
        }
        if (!$('input[name=RB_MUSCLE_' + type + id + ']:checked').val()) {
            alert("[肌肉張力]未填")
            return false;
        }
        if (!$('input[name=RB_FS_' + type + id + ']:checked').val()) {
            alert("[解胎便]未填")
            return false;
        }
        if (!$('input[name=RB_URINE_' + type + id + ']:checked').val()) {
            alert("[解小便]未填")
            return false;
        }
        if (!$('input[name=RB_INCU_' + type + id + ']:checked').val()) {
            alert("[保溫箱]未填")
            return false;
        }
        return true;
    }

    function sendForm(url, type, isAdd) {
        $("input[name=TYPE]").val(type);

        if (check(type, isAdd)) {
            $.ajax({
                url: '../BabyBorn/' + url,
                type: 'post',
                dataType: 'json',
                data: $('form').serialize(),
                success: function (data) {
                    if (Number(data) > 0) {
                        if (isAdd) {
                            alert('新增成功');
                        } else {
                            alert('修改成功');
                        }
                    } else {
                        if (isAdd) {
                            alert('新增失敗');
                        } else {
                            alert('修改失敗');
                        }
                    }
                    window.location.href = '../Obstetrics/Index?active=NB7HR_List';
                },
                failure: function (errMsg) {
                    if (isAdd) {
                        alert('新增失敗');
                    } else {
                        alert('修改失敗');
                    }
                    window.location.href = '../Obstetrics/Index?active=NB7HR_List';
                }
            });
        }
    };

    function Delete(type, hour) {
        if (confirm('確認刪除第' + hour + '小時紀錄?')) {
            $("input[name=TYPE]").val(type);
            $.ajax({
                url: '../BabyBorn/Del_NB7HR',
                type: 'post',
                dataType: 'json',
                data: $('form').serialize(),
                success: function (data) {
                    if (Number(data) > 0) {
                        alert('刪除成功');
                    } else {
                        alert('刪除失敗');
                    }
                    window.location.href = '../Obstetrics/Index?active=NB7HR_List';
                },
                failure: function (errMsg) {
                    alert('刪除失敗');
                    window.location.href = '../Obstetrics/Index?active=NB7HR_List';
                }
            });
        }
    }
</script>

