<script type="text/javascript">
    $(document).ready(function () {
        //自動調整頁面高度   //分頁選項高度
        $('input[type=text], textarea').each(function () {
            $(this).replaceWith("<span>" + $(this).val().replace(/\|/g, ',') + "</span>");
        });
        $('input[type=radio], input[type=checkbox]').each(function () {
            if (this.checked)
                $(this).remove();
            else
                $(this.parentElement).remove();
        });
        $('img').remove();
    });

    //驗證欄位_數字
    function numcheck(obj) {
        if (obj.value.indexOf('.') > -1) {
            var re = /^[0-9]+\.[0-9]*$/;
        }
        else {
            var re = /^[0-9]+$/;
        }
        if (!re.test(obj.value) && obj.value != "") {
            alert("只能輸入數字");
            $(obj).val('');
            return false;
        }
        else
            return true;
    }

    function set_for_rb(rb_name, dvalue) {
        if (dvalue != '') {
            dvalue = dvalue.replace('&lt;', '<').replace('&gt;', '>');
            $('input[name=' + rb_name + ']').each(function () {
                if ($(this).val() == dvalue)
                    $(this).click();
            });
        }
    }

    function set_for_ck(ck_name, val) {
        if (val != '') {
            var default_val = val.split(",");
            var ck = document.getElementsByName(ck_name);
            for (var i = 0; i < default_val.length; i++) {
                for (var j = 0; j < ck.length; j++) {
                    if (default_val[i] == ck[j].value || (ck_name = "param_allergy_other_other" && ck[j].value.indexOf("麈") > -1))
                        $(ck[j]).click();
                }
            }
        }
    }

    function set_for_txt(txt_name, dvalue) {
        if (dvalue != '') {
            var txt = document.getElementsByName(txt_name);
            var default_val = dvalue.split(",");
            if (txt.length > 1) {
                for (var i = 0; i < txt.length; i++) {
                    $(txt[i]).val(default_val[i]);
                }
            }
            else
                $(txt).val(dvalue);
        }
    }

    function OnclickRB_FEED(value) {
        if (!value)
            value = '';

        if ($("input[name =RB_FEED" + value + "]:checked").val() == "0") {
            $("input[name =CK_FEED_K" + value + "]").attr("disabled", true);
            $("input[name =TXT_FEED_MB" + value + "]").attr("disabled", true);
            $("input[name =TXT_FEED_MF" + value + "]").attr("disabled", true);
            $("input[name =TXT_FEED_MA" + value + "]").attr("disabled", true);
        } else {
            $("input[name =CK_FEED_K" + value + "]").attr("disabled", false);
            $("input[name =TXT_FEED_MB" + value + "]").attr("disabled", false);
            $("input[name =TXT_FEED_MF" + value + "]").attr("disabled", false);
            $("input[name =TXT_FEED_MA" + value + "]").attr("disabled", false);
        }
    }
</script>

